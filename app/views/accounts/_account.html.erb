<div id="<%= dom_id account %>">
  <p>
    <strong>Account:</strong>
    <%= account.account_id %>
  </p>

  <p>
    <strong>Available:</strong>
    <%= account.available %>
  </p>

  <p>
    <strong>Current:</strong>
    <%= account.current %>
  </p>

  <p>
    <strong>Limit:</strong>
    <%= account.limit %>
  </p>

  <p>
    <strong>Last four:</strong>
    <%= account.last_four %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= account.name %>
  </p>

  <p>
    <strong>Official name:</strong>
    <%= account.official_name %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= account.account_type %>
  </p>

  <p>
    <strong>Subtype:</strong>
    <%= account.subtype %>
  </p>

</div>
