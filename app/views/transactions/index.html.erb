<p style="color: green"><%= notice %></p>

<h1>Transactions</h1>

<%= link_to 'Home', root_path%>

<div id="transactions">
  <% @transactions[0..20].each do |transaction| %>
    <%= render transaction %>
    <p>
      <%= link_to "Show this transaction", transaction %>
    </p>
  <% end %>
</div>

<%= link_to "New transaction", new_transaction_path %>
